<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="jo.carinfo">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <service android:name=".LocationUpd" android:process=":my_process"/>
        <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyBPfZq8Zh4XBk_zduLwnJZddekpMQ9kHk0"/>
        <activity android:name=".StationsActivity"
                android:process=":my_process"></activity>
        <activity
                android:name=".FuelUsageGraph"
                android:screenOrientation="landscape"></activity>
        <activity android:name=".OilEntryActivity"></activity>
        <activity android:name=".CarEntries"></activity>
        <activity android:name=".simple_name"></activity>
        <activity android:name=".FuelEntryActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".MainActivity"
                android:process=":my_process">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>